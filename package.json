{
  "name": "pettube-server",
  "version": "0.1.0",
  "engines": {
    "node": "14.15.4"
  },
  "scripts": {
    "start": "node server",
    "dev:server": "nodemon server",
    "dev:client": "cd client && npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "babel --delete-dir-on-start --out-dir dist --copy-files --ignore \"**/__tests__/**,**/__mocks__/**\" --no-copy-ignored server/src",
    "save": "prisma migrate --experimental save",
    "up": "prisma migrate --experimental up",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "postinstall": "cd client && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "author": "Biswajit Mondal",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "2.12.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "google-auth-library": "^6.1.3",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.7.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@prisma/cli": "2.12.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
